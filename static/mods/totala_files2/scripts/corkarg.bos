AimPrimary(heading, pitch)
{
	signal 2;
	set-signal-mask 2;
	Static_Var_3 = 1;
	Static_Var_4 = 1;
	Static_Var_9 = heading;
	Static_Var_10 = 0 - pitch;
	if( !Static_Var_5 )
	{
		turn stick to y-axis heading speed <60.>;
	}
	turn torso to y-axis heading speed <60.>;
	turn larm to x-axis <0.> - pitch speed <50.>;
	turn rarm to x-axis <0.> - pitch speed <50.>;
	Static_Var_10 = 0 - pitch;
	if( Static_Var_10 <= -32768 )
	{
		turn rcannon to y-axis Static_Var_10 speed <55.>;
		turn lcannon to y-axis <0.> - Static_Var_10 speed <55.>;
	}
	if( Static_Var_10 > -32768 )
	{
		turn rcannon to y-axis <0.> speed <55.>;
		turn lcannon to y-axis <0.> speed <55.>;
	}
	wait-for-turn torso around y-axis;
	wait-for-turn lshoulder around x-axis;
	wait-for-turn rshoulder around x-axis;
	wait-for-turn rcannon around x-axis;
	wait-for-turn lcannon around x-axis;
	start-script RestorePrimary();
	return (1);
}

