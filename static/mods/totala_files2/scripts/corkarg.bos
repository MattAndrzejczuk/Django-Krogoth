	Static_Var_9 = 0;
	Static_Var_10 = 0;
	turn stick to y-axis <0.> speed <50.>;
	turn torso to y-axis <0.> speed <50.>;
	turn larm to x-axis <0.> speed <40.>;
	turn rarm to x-axis <0.> speed <40.>;
	turn lcannon to y-axis <0.> speed <65.>;
	turn rcannon to y-axis <0.> speed <65.>;
	wait-for-turn torso around y-axis;
	wait-for-turn rarm around x-axis;
	wait-for-turn rcannon around y-axis;
	Static_Var_4 = 0;
	if( Static_Var_3 == 1 )
	{
		if( Static_Var_5 == 0 AND Static_Var_6 == 0 )
		{
			Static_Var_3 = 0;
			return (0);
		}
