AimPrimary(heading, pitch)
{
	var Func_Var_3, Func_Var_4, Func_Var_5;
	signal 2;
	set-signal-mask 2;
	Static_Var_2 = 1;
	turn calcarm to x-axis <0.> - pitch now;
	Func_Var_4 = get PIECE_Y(calcpoint);
	Func_Var_3 = get PIECE_Y(calcflare);
	if( Func_Var_4 >= Func_Var_3 )
	{
		Func_Var_5 = ( Func_Var_4 - Func_Var_3 ) / 300 + 360;
		turn ruparm to y-axis Func_Var_5 speed <40.>;
		turn luparm to y-axis <0.> - Func_Var_5 speed <40.>;
	}
	if( Func_Var_4 < Func_Var_3 )
	{
		turn ruparm to y-axis <0.> speed <40.>;
		turn luparm to y-axis <0.> speed <40.>;
	}
	turn torsoup to y-axis heading speed <120.>;
	wait-for-turn torsoup around y-axis;
	turn ruparm to x-axis <0.> - pitch speed <90.>;
	turn luparm to x-axis <0.> - pitch speed <90.>;
	turn rightturret to x-axis <0.> - pitch speed <70.>;
	turn leftturret to x-axis <0.> - pitch speed <70.>;
	wait-for-turn ruparm around x-axis;
	wait-for-turn luparm around x-axis;
	wait-for-turn ruparm around y-axis;
	wait-for-turn luparm around y-axis;
	wait-for-turn rightturret around x-axis;
	wait-for-turn leftturret around x-axis;
	start-script restoreposition();
	return (1);
}

